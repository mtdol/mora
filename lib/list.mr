data List a := Cons val :: a, next :: List a | Null;

op : := Cons;

fn null v {return v = Null;}

fn head v {return
    if null v then 
        error "`head`: Cannot deref Null."
    else
        v.val
;}

fn tail v {return
    if null v then 
        error "`tail`: Cannot deref Null."
    else
        v.next
;}
